{
    "name": "mortezamasumi/method-overrider",
    "description": "A PHP package to override methods. it is a clone from Christoph Rumpel package",
    "version": "v1.2.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Morteza Masumi",
            "email": "morteza.masumi@gmail.com"
        }
    ],
    "keywords": [
        "php",
        "overriding",
        "methods"
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.17.3",
        "orchestra/testbench": "^9.6",
        "pestphp/pest": "^3.0.7",
        "pestphp/pest-plugin-type-coverage": "^3.0",
        "phpstan/phpstan": "^1.12.4",
        "rector/rector": "^1.2.5",
        "symfony/var-dumper": "^7.1.4"
    },
    "autoload": {
        "psr-4": {
            "Mortezamasumi\\MethodOverrider\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
